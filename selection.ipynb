        "def sort(nums):\n",
        "  for i in range(5):\n",
        "    minpos=i\n",
        "    for j in range(i,6):\n",
        "      if nums[j]<nums[minpos]:\n",
        "        minpos=j\n",
        "    temp=nums[i]\n",
        "    nums[i]=nums[minpos]\n",
        "    nums[minpos]=temp\n",
        "\n",
        "\n",
        "    print(nums)\n",
        "nums=[100,-1,5,78,4,2]\n",
        "sort(nums)\n",
        "\n",
        "print(nums)"
      ]
